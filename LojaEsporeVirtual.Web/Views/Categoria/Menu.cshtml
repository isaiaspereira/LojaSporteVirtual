@model System.Collections.Generic.IEnumerable<string>


@Html.ActionLink("Todos os Produto", "ListaProduto", "Vitrine", null, new { @class = "btn  btn-outline-dark btn-block btn-lg" })


@foreach (var links in Model)
{
    @Html.RouteLink(links, new
{
    controller = "Vitrine",
    action = "ListaProduto",
    categoria = links,
    pagina = 1
}, new
{
    @class = "btn  btn-default btn-block btn-lg"
        + (links == ViewBag.CategoriaSelecionada ? " btn btn-dark" : "")
})
}